@page "/Tariffs/{MeterTypeText}"
@inherits FluxorComponent


<ExtendedPageTitle>@MeterTypeText Tariffs</ExtendedPageTitle>

<MudStack Class="mt-5"> 
    
    <MudPaper Elevation="2" Class="p-2">
        <MeterTariffsTable MeterType="_meterType"></MeterTariffsTable>
    </MudPaper>
</MudStack>

@code {
    [Parameter]
    public string MeterTypeText { get; set; }

    
    protected override void OnParametersSet()
    {
        _meterType = MeterTypeText.eStringToEnum<MeterType>();
    }

    private MeterType _meterType;
}
