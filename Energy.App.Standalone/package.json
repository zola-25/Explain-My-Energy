{
  "name": "explain-my-energy",
  "version": "1.0.0",
  "description": "Understand your energy consumption and carbon emissions, and forecast your future energy costs based on seasonal changes and tariff increases",
  "scripts": {
    "singleCompileSass": "sass ClientSide/Sass/:wwwroot/css/",
    "watchSass": "sass --watch ClientSide/Sass/:wwwroot/css/",
    "buildPowershell": "pwsh -File src/attributionAndLicensing/BuildAndDistributeThirdPartyAttribution.ps1 -finalOutputPath src/views/AttributionAndLicensing.html -attribsCssPath '..scss/app/attribs.scss'",
    "buildCreditsDoc": "node src/attributionAndLicensing/GenerateCreditsDoc.js",
    "buildNpmCredits": "node src/attributionAndLicensing/NpmLicenseGeneration/NpmCreditsBuilder.js --packagesJsonFolder . --generatedHtmlDocumentPath src/views/NpmCreditsPartial.html",
    "buildFinalCreditsDoc": "node src/attributionAndLicensing/FinalCreditsDocGeneration/FinalCreditsDocGeneration.js --projectRootPath . --finalGeneratedCreditsHtmlDocPath src/views/Credits.html",
    "buildNugetCredits": "node src/attributionAndLicensing/NugetLicenseGeneration/NugetCreditsBuilder.js --projectCsprojPath ./Energy.App.Standalone.csproj --generatedHtmlDocumentPath src/views/NugetCreditsPartial.html",
    "buildDev": "webpack --config ClientSide/webpack.dev.config.js",
    "buildHtmlBundleDev": "webpack --config src/webpack.htmlb.dev.config.js",
    "buildHtmlBundleProd": "webpack --config src/webpack.htmlb.prod.config.js",
    "buildProd": "webpack --config ClientSide/webpack.prod.config.js",
    "test": "mocha --recursive --grep '.*' src/test",
    "buildFileDev": "node src/build.js --development",
    "buildFileProd": "node src/build.js --production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zola-25/Explain-My-Energy.git"
  },
  "browserslist": [
    "defaults and supports es6-module"
  ],
  "type": "module",
  "keywords": [],
  "author": "zola-25",
  "config": {
    "fullApplicationName": "Explain My Energy",
    "licenseFilePath": "../LICENSE"
  },
  "homepage": "https://github.com/zola-25/Explain-My-Energy/blob/master/README.md",
  "private": true,
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/dompurify": "^3.0.2",
    "@types/node": "^20.4.10",
    "@types/webpack": "^5.28.1",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "async-exit-hook": "^2.0.1",
    "autoprefixer": "^10.4.15",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "ejs": "^3.1.9",
    "eslint": "^8.47.0",
    "file-loader": "^6.2.0",
    "grunt": "^1.6.1",
    "grunt-contrib-clean": "^2.0.1",
    "grunt-contrib-concat": "^2.1.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-webpack": "^6.0.0",
    "handlebars": "^4.7.8",
    "html-bundler-webpack-plugin": "^2.8.0",
    "html-webpack-plugin": "^5.5.3",
    "isomorphic-dompurify": "^1.8.0",
    "license-checker-rseidelsohn": "^4.2.6",
    "license-webpack-plugin": "^4.0.2",
    "marked": "^7.0.3",
    "mini-css-extract-plugin": "^2.7.6",
    "minimist": "^1.2.8",
    "mocha": "^10.2.0",
    "node-html-parser": "^6.1.5",
    "postcss": "^8.4.28",
    "postcss-loader": "^7.3.3",
    "sass-loader": "^13.3.2",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "yargs": "^17.7.2"
  },
  "dependencies": {
    "@amcharts/amcharts5": "^5.4.2",
    "lodash": "^4.17.21"
  }
}
